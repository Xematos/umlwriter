{
    "name": "bartlett/umlwriter",
    "description": "Create UML class diagrams from your PHP source.",
    "keywords": ["UML", "Graphviz", "PlantUML"],
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "http://php5.laurent-laville.org/umlwriter/",
    "support": {
        "source": "https://github.com/llaville/umlwriter",
        "issues": "https://github.com/llaville/umlwriter/issues"
    },
    "require": {
        "php": "^7.1.3",
        "goaop/parser-reflection": "^2.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "jawira/plantuml": "^1.28"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/llaville/graphviz"
        }
    ],
    "authors": [
        {
            "name": "Laurent Laville",
            "email": "pear@laurent-laville.org",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "bin": [
        "bin/umlwriter"
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bartlett\\UmlWriter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bartlett\\UmlWriter\\Tests\\": "tests/"
        }
    }
}
