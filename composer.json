    },
    "require-dev": {
        "bartlett/php-reflect": "3.0.0-RC1",
        "andrewsville/php-token-reflection": "~1.4"
    },
    "suggest": {
        "bartlett/php-reflect": "Reverse-engine compatible solution 1"
        "andrewsville/php-token-reflection": "Reverse-engine compatible solution 2"
    },
    "authors": [
        {
            "name": "Laurent Laville",
            "email": "pear@laurent-laville.org",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "autoload": {
        "psr-0": {
            "Bartlett\\": "src/"
        }
    }
}
